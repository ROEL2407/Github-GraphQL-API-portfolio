<!DOCTYPE html>
<html lang="en">
  <%- include('./components/head.ejs') -%>
  <body>
    <section>
      <% projects.forEach(function(item){%>
      <article>
        <h2><%- item.node.name %></h2>
        <p><%- item.node.description %></p>
        <!-- details  -->
        <p>Details</p>
        <% if (item.node.homepageUrl !== null) { %>
        <p><a href="<%- item.node.homepageUrl %>">Live demo</a></p>
        <% } %>
      </article>
      <%}) %>
    </section>
  </body>
</html>
